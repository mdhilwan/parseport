    "prod": "npm run prod:app && npx -y concurrently -c \"auto\" \"next start\" \"npm:prod:api\"",
    "prod:app": "next build",
    "prod:api": "NODE_ENV=production npx -y nodemon server/api.js",
    "pm2": "pm2",
    "live": "pm2 start pm2.json",
    "lint": "next lint",
    "prepare": "husky"
  },
